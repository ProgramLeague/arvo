import axiomatic_equality.

axiom sigT : (A : Type) -> (A -> Type) -> Type.

axiom existT : (A : Type) -> (B : A -> Type) -> (a : A) -> B a -> sigT A B.

axiom projT1 : (A : Type) -> (B : A -> Type) -> sigT A B -> A.

axiom projT2 : (A : Type) -> (B : A -> Type) -> (s : sigT A B) -> B (projT1 A B s).

